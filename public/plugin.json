{
  "pluginName": "列表过滤器",
  "description": "智能列表管理工具，支持关键词快速匹配和剪切板复制功能",
  "author": "开发者",
  "version": "1.0.0",
  "main": "index.html",
  "preload": "preload/services.js",
  "logo": "logo.png",
  "platform": ["win32", "darwin", "linux"],
  "development": {
    "main": "http://localhost:5173"
  },
  "features": [
    {
      "code": "list-manager",
      "explain": "列表管理器 - 管理你的列表项目",
      "cmds": [
        "list"
      ]
    },
    {
      "code": "list-search",
      "explain": "搜索列表项并复制到剪切板",
      "cmds": [
        {
          "type": "regex",
          "label": "搜索列表项",
          "match": "/^wj\\s+(.+)$/",
          "minLength": 1
        }
      ]
    }
  ]
}
